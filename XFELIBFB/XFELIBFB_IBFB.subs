#################################################################################
### IBFB Cavity BPMs                                                         ####
#################################################################################

file ibfbcav.template {
   pattern  {DEV              SRV          READ_OFFSET  }
            {IBFBYUP          IBFBYUP               32  }
            {IBFBYDOWN        IBFBYDOWN             32  }
            {IBFBXUP          IBFBXUP               32  }
            {IBFBXDOWN        IBFBXDOWN             32  }
}

file ibfbcavb.template {
   pattern  {DEV              SRV          READ_OFFSET  }
            {IBFBYCOL         IBFBYCOL              16  }
}


